result_list = []

for num in range(1, 101):
    cube = num ** 3
    if cube % 4 == 0 or cube % 5 == 0:
        result_list.append(num)

print(result_list)
result_list = []
num = 1

while num <= 100:
    cube = num ** 3
    if cube % 4 == 0 or cube % 5 == 0:
        result_list.append(num)
    num += 1

print(result_list)
